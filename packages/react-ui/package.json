{
	"name": "@packages/react-components",
	"version": "0.0.2",
	"type": "module",
	"license": "MIT",
	"author": "ph.duy113@gmail.com",
	"description": "THis is a UI component library for building React application",
	"keywords": [
		"react",
		"components",
		"ui"
	],
	"scripts": {
		"type-checks": "tsc --noEmit",
		"build": " vite build",
		"prettier": "prettier . --write --cache",
		"lint": "eslint .",
		"preview": "vite preview",
		"prepack": "clean-package",
		"postpack": "clean-package restore",
		"prepublishOnly": "npm run build",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		},
		"./components": {
			"types": "./dist/components/index.d.ts",
			"default": "./dist/components/index.js"
		},
		"./themes/*.css": "./dist/themes/*.css"
	},
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/phamhaduy95/UI-components.git",
		"directory": "packages/react-ui"
	},
	"bugs": {
		"url": "https://github.com/phamhaduy95/react-component-lib/issues"
	},
	"homepage": "https://github.com/phamhaduy95/react-component-lib#readme",
	"files": [
		"dist"
	],
	"dependencies": {
		"@ark-ui/react": "^5.30.0",
		"@radix-ui/react-icons": "^1.3.2",
		"react": "^19.2.4",
		"react-dom": "^19.2.4"
	},
	"devDependencies": {
		"@chromatic-com/storybook": "^5.0.0",
		"@storybook/addon-a11y": "^10.2.3",
		"@storybook/addon-docs": "^10.2.3",
		"@storybook/addon-vitest": "^10.2.3",
		"@storybook/react-vite": "^10.2.3",
		"@tailwindcss/vite": "^4.1.18",
		"@types/node": "^22.19.7",
		"@types/react": "^19.2.10",
		"@types/react-dom": "^19.2.3",
		"@typescript-eslint/eslint-plugin": "^8.54.0",
		"@typescript-eslint/parser": "^6.21.0",
		"@vitejs/plugin-react-swc": "^3.11.0",
		"classnames": "^2.5.1",
		"clean-package": "^2.2.0",
		"eslint": "^9.39.2",
		"eslint-plugin-react-hooks": "^5.2.0",
		"eslint-plugin-react-refresh": "^0.4.26",
		"eslint-plugin-storybook": "^10.2.3",
		"glob": "^11.1.0",
		"playwright": "^1.58.1",
		"storybook": "^10.2.3",
		"tailwindcss": "^4.1.18",
		"vite": "^7.3.1",
		"vite-plugin-dts": "^4.5.4",
		"vite-plugin-lib-inject-css": "^2.2.2",
		"vitest": "^4.0.18"
	},
	"peerDependencies": {
		"react": ">=18.0.0",
		"react-dom": ">=18.0.0"
	},
	"sideEffects": [
		"**/*.css"
	]
}
