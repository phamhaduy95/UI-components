---
import { getEntry, render } from 'astro:content';
import DocumentLayout from '@layouts/DocumentLayout.astro';

const entry = await getEntry('components', 'TextInput');
if (!entry) {
	throw new Error('Could not find TextInput component documentation');
}

const { Content, headings } = await render(entry);
---

<DocumentLayout title="TextInput Component | React UI" headings={headings}>
	<Content />
</DocumentLayout>
