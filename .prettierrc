{
	"useTabs": true,
	"singleQuote": true,
	"trailingComma": "none",
	"semi": true,
	"printWidth": 100,
	"tabWidth": 2,
	"bracketSameLine": false,

	"plugins": ["prettier-plugin-astro", "prettier-plugin-tailwindcss"],
	"overrides": [
		{
			"files": "*.astro",
			"options": {
				"parser": "astro"
			}
		}
	]
}
